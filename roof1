class Roof{
    constructor(x,y,height,width){
        this.body = Bodies.rectangle(x,y,width,heifht,{isStatic:true}));
        this.width = width;
        this.height = height;
        world.add(world,this.body);

    }
    display(){
    push();
    rectMode(CENTER);
    fill(128,128,128);
    rect(this.body.position.x,this.body.postion.y,this.width,this.height);
pop();
    }
}